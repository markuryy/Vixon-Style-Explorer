{
    "manifest_version": 3,
    "name": "Vixon Style Explorer",
    "version": "1.0",
    "description": "Improves the UI for navigating Vixon's Lora styles on CivitAI.",
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": ["https://civitai.com/*"],
        "js": ["content.js"]
      }
    ],
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
  }